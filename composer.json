{
    "name": "shopery/view",
    "description": "Helpers to work with views",
    "license": "MIT",
    "keywords": [
    ],
    "authors": [
        {
            "name": "Berny Cantos",
            "email": "be@rny.cc"
        }
    ],
    "require": {
        "beberlei/assert": ">=1.5,<3",
        "php": ">=5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7",
        "php": ">=5.5"
    },
    "autoload": {
        "psr-4": {
            "Shopery\\View\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shopery\\View\\Test\\": "tests"
        }
    },
    "type": "library",
    "minimum-stability": "dev",
    "config": {
        "bin-dir": "bin",
        "vendor-dir": "lib"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
